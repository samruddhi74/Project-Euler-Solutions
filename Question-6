#include<iostream>
#include<vector>
#include<set>
#include<map>
#include<algorithm>
#include<climits>
#include<string>

using namespace std;


void solve(){
ll n = 100;
ll sumOfSquare = (n*(n+1)*(2*n+1))/6;
ll sum = (n*(n+1))/2;
cout<< abs(sumOfSquare- sum*sum);
}

int main(){
ios_base::sync_with_stdio(0);
cin.tie(0);cout.tie(0);

solve();

return 0;
}
